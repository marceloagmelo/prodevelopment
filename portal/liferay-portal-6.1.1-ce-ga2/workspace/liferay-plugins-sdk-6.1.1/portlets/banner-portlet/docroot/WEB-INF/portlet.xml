<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>bannerrotativo</portlet-name>
		<display-name>BannerRotativo</display-name>
		<portlet-class>
			br.com.prodevelopment.banner.rotativo.portlet.BannerRotativoPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/bannerrotativo/view.jsp</value>
		</init-param>
		<init-param>
			<name>naoConfigurado-jsp</name>
			<value>/html/naoConfigurado.jsp</value>
		</init-param>
		<init-param>
			<name>viewGrande2-jsp</name>
			<value>/html/bannerrotativo/viewGrande2.jsp</value>
		</init-param>
		<init-param>
			<name>viewGrande3-jsp</name>
			<value>/html/bannerrotativo/viewGrande3.jsp</value>
		</init-param>
		<init-param>
			<name>intervalo_default</name>
			<value>4</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>
			content/bannerrotativo/Language
		</resource-bundle>
		<portlet-info>
			<title>BannerRotativo</title>
			<short-title>BannerRotativoPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>listabanner</portlet-name>
		<display-name>ListaBanner</display-name>
		<portlet-class>
			br.com.prodevelopment.banner.lista.portlet.ListaBannerPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/listabanner/view.jsp</value>
		</init-param>
		<init-param>
			<name>naoConfigurado-jsp</name>
			<value>/html/naoConfigurado.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>
			content/listabanner/Language
		</resource-bundle>
		<portlet-info>
			<title>ListaBanner</title>
			<short-title>ListaBannerPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>