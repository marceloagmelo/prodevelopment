<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">

	<portlet>
		<portlet-name>rasecbuscaportlet</portlet-name>
		<display-name>BuscaPortlet</display-name>
		<portlet-class>
			br.com.rasecmadeiras.busca.portlet.BuscaPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/busca/view.jsp</value>
		</init-param>
		<init-param>
			<name>naoConfigurado-jsp</name>
			<value>/html/naoConfigurado.jsp</value>
		</init-param>
		<init-param>
			<name>nome-parametro-busca</name>
			<value>parambusca</value>
		</init-param>
		<init-param>
			<name>nome-param-id</name>
			<value>id</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/busca/Language</resource-bundle>
		<portlet-info>
			<title>BuscaPortlet</title>
			<short-title>BuscaPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>rasecresultadobuscaportlet</portlet-name>
		<display-name>ResultadoBuscaPortlet</display-name>
		<portlet-class>
			br.com.rasecmadeiras.busca.resultado.portlet.ResultadoBuscaPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/busca/resultado/view.jsp</value>
		</init-param>
		<init-param>
			<name>naoConfigurado-jsp</name>
			<value>/html/naoConfigurado.jsp</value>
		</init-param>
		<init-param>
			<name>nome-parametro-busca</name>
			<value>parambusca</value>
		</init-param>
		<init-param>
			<name>nome-param-id</name>
			<value>id</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>
			content/busca/resultado/Language
		</resource-bundle>
		<portlet-info>
			<title>ResultadoBuscaPortlet</title>
			<short-title>ResultadoBuscaPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>