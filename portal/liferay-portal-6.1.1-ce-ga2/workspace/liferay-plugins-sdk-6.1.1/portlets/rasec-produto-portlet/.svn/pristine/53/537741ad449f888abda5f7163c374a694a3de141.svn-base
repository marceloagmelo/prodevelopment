var {
  background: transparent;
  border: 0 none;
  font-size: 100%;
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  vertical-align: top; }

ul.tabs2 {
  margin: 0;
  padding: 0;
  width: 100%;
  overflow: hidden;
  font-family: Arial, Helvetica, sans-serif; }

ul.tabs2 li {
  float: left;
  margin: 0 0 0 -24px;
  padding: 0;
  height: 37px;
  position: relative;
  font-weight: bold;
  list-style: none; }

ul.tabs2 li:first-child {
  margin-left: 0; }

ul.tabs2 li a {
  text-decoration: none;
  color: #fff;
  display: block;
  font-size: 12px;
  line-height: 15px;
  height: 37px;
  background: url(../../images/destaque/tab4_l.png) left top no-repeat;
  padding: 0 0 0 22px; }

ul.tabs2 li a span {
  display: block;
  background: url(../../images/destaque/tab4_r.png) right top no-repeat;
  padding: 0 5px 0 0; }

ul.tabs2 li a span span {
  text-align: center;
  width: 104px;
  background: url(../../images/destaque/tab4_t.png) right top repeat-x;
  padding: 11px 6px 11px 0px; }

ul.tabs2 li:first-child a {
  background: url(../../images/destaque/tab3_l.png) left top no-repeat;
  padding: 0 0 0 5px; }

ul.tabs2 li:first-child a span {
  background: url(../../images/destaque/tab3_r.png) right top no-repeat;
  padding: 0 22px 0 0; }

ul.tabs2 li:first-child a span span {
  width: 150px;
  background: url(../../images/destaque/tab3_t.png) right top repeat-x;
  padding: 11px 0px 11px 10px; }

ul.tabs2 li.active a {
  background-position: left -37px !important; }

ul.tabs2 li.active a span {
  background-position: right -37px !important; }

ul.tabs2 li.active {
  z-index: 111 !important; }

ul.tabs2 li.active a:hover {
  color: #fff; }

ul.tabs2 li a:hover {
  color: #f93434; }

.containerDestaqueProd {
  border-radius: 0 0 6px 6px;
