  background: url(../../images/destaque/bg-2.gif) left top repeat-x #017fba;
  overflow: hidden;
  margin-bottom: 10px; }

.conteudoDestaqueProd {
  background: url(../../images/destaque/bg-2.gif) left top repeat-x #017fba;
  padding: 18px 18px 14px 11px;
  border-radius: 0 0 6px 6px; }

.color-1 {
  color: #d8e138; }

.color-2 {
  color: #cf001c; }

.color-3 {
