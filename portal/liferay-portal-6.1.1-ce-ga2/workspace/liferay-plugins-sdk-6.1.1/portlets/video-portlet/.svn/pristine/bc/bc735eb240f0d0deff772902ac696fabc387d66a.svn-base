<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>

	<portlet>
		<portlet-name>videodestaqueportlet</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>br.com.prodevelopment.video.destaque.action.ConfigurationActionImpl</configuration-action-class>
		<instanceable>false</instanceable>
		<header-portlet-css>
			/css/videodestaque/main.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/videodestaque/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			videodestaqueportlet-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>listavideoportlet</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>br.com.prodevelopment.video.lista.action.ConfigurationActionImpl</configuration-action-class>
		<instanceable>false</instanceable>
		<header-portlet-css>
			/css/listavideo/main.css
		</header-portlet-css>
		<footer-portlet-javascript>
			/js/listavideo/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>listavideoportlet-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>videorotativoportlet</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>br.com.prodevelopment.video.rotativo.action.ConfigurationActionImpl</configuration-action-class>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/videorotativo/main.css</header-portlet-css>
		<header-portlet-javascript>/js/videorotativo/mootools.v1.1.js</header-portlet-javascript>
		<header-portlet-javascript>/js/videorotativo/shCore.js</header-portlet-javascript>
		<header-portlet-javascript>/js/videorotativo/iCarousel.js</header-portlet-javascript>
		<css-class-wrapper>
			videorotativoportlet-portlet
		</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
