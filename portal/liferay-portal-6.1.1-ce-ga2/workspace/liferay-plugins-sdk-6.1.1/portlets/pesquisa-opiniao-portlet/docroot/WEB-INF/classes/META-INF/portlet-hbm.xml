<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="br.com.prodevelopment.pesquisaopiniao.model.Pergunta" />
	<import class="br.com.prodevelopment.pesquisaopiniao.model.Pesquisa" />
	<import class="br.com.prodevelopment.pesquisaopiniao.model.Pesquisa_Usuario" />
	<import class="br.com.prodevelopment.pesquisaopiniao.model.Resposta" />
	<import class="br.com.prodevelopment.pesquisaopiniao.model.Resposta_Usuario" />
	<class name="br.com.prodevelopment.pesquisaopiniao.model.impl.PerguntaImpl" table="PO_Pergunta">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="descricao" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dataInclusao" type="org.hibernate.type.TimestampType" />
		<property name="dataAlteracao" type="org.hibernate.type.TimestampType" />
		<property name="pesquisaId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdInclusao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdAlteracao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="br.com.prodevelopment.pesquisaopiniao.model.impl.PesquisaImpl" table="PO_Pesquisa">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="titulo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="descricao" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dataInclusao" type="org.hibernate.type.TimestampType" />
		<property name="dataAlteracao" type="org.hibernate.type.TimestampType" />
		<property name="ativa" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdInclusao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdAlteracao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="br.com.prodevelopment.pesquisaopiniao.model.impl.Pesquisa_UsuarioImpl" table="PO_Pesquisa_Usuario">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="nome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dataInclusao" type="org.hibernate.type.TimestampType" />
		<property name="pesquisaId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="br.com.prodevelopment.pesquisaopiniao.model.impl.RespostaImpl" table="PO_Resposta">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="descricao" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dataInclusao" type="org.hibernate.type.TimestampType" />
		<property name="dataAlteracao" type="org.hibernate.type.TimestampType" />
		<property name="perguntaId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdInclusao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userIdAlteracao" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="br.com.prodevelopment.pesquisaopiniao.model.impl.Resposta_UsuarioImpl" table="PO_Resposta_Usuario">
		<cache usage="read-write" />
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="dataInclusao" type="org.hibernate.type.TimestampType" />
		<property name="pesquisaUsuarioId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="respostaId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>