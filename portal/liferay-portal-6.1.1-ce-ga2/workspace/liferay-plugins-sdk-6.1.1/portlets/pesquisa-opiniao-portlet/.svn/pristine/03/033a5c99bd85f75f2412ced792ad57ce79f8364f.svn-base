package br.com.prodevelopment.pesquisaopiniao.vo;

import java.util.List;
import java.util.Locale;
import java.util.Map;

import br.com.prodevelopment.pesquisaopiniao.Constantes;

public class PesquisaVO {

	private String id;
	private String titulo;
	private Map<Locale, String> tituloMap;
	private String tituloCorrente;
	private String descricao;
	private Map<Locale, String> descricaoMap;
	private String descricaoCorrente;
	private String ativa;
	private List<PerguntaVO> perguntas;
	private PesquisaUsuarioVO pesquisaUsuarioVO;
	
	public PesquisaVO() {
		this.id = "";
		this.titulo = "";
		this.tituloCorrente = "";
		this.tituloMap = null;
		this.descricao = "";
		this.descricaoCorrente = "";
		this.descricaoMap = null;
		this.perguntas = null;
		this.pesquisaUsuarioVO = null;
		this.ativa = "";
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	public List<PerguntaVO> getPerguntas() {
		return perguntas;
	}

	public void setPerguntas(List<PerguntaVO> perguntas) {
		this.perguntas = perguntas;
	}

	public PesquisaUsuarioVO getPesquisaUsuarioVO() {
		return pesquisaUsuarioVO;
	}

	public void setPesquisaUsuarioVO(PesquisaUsuarioVO pesquisaUsuarioVO) {
		this.pesquisaUsuarioVO = pesquisaUsuarioVO;
	}

	public String getAtiva() {
		return ativa;
	}

	public void setAtiva(String ativa) {
		this.ativa = ativa;
	}

	public Map<Locale, String> getTituloMap() {
		return tituloMap;
	}

	public void setTituloMap(Map<Locale, String> tituloMap) {
		this.tituloMap = tituloMap;
	}

	public Map<Locale, String> getDescricaoMap() {
		return descricaoMap;
	}

	public void setDescricaoMap(Map<Locale, String> descricaoMap) {
		this.descricaoMap = descricaoMap;
	}

	public String getTituloCorrente() {
		return tituloCorrente;
	}

	public void setTituloCorrente(String tituloCorrente) {
		this.tituloCorrente = tituloCorrente;
	}

	public String getDescricaoCorrente() {
		return descricaoCorrente;
	}

	public void setDescricaoCorrente(String descricaoCorrente) {
		this.descricaoCorrente = descricaoCorrente;
	}
	
}
