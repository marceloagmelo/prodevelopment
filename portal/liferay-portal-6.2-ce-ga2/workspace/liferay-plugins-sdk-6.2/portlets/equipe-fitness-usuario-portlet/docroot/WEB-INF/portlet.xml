<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>usuario-lista</portlet-name>
		<display-name>Usuario Lista</display-name>
		<portlet-class>
			br.com.prodevelopment.equipefitness.portlet.usuario.lista.action.UsuarioListaPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/lista/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/html/lista/config.jsp</value>
		</init-param>
		<init-param>
			<name>naoConfigurado-jsp</name>
			<value>/html/portlet_not_setup.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.lista.Language</resource-bundle>
		<portlet-info>
			<title>Usuario Lista</title>
			<short-title>Usuario Lista</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>